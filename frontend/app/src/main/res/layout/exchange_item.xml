<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/lih_iExBar2"
        android:layout_width="800dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:weightSum="41">

        <TextView
            android:id="@+id/tv_iExOrderTime"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="채결시간"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/tv_iExCoin"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="코인명"
            android:textColor="#000000" />


        <TextView
            android:id="@+id/tv_iExType"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="종류"
            android:textColor="#000000" />


        <TextView
            android:id="@+id/tv_iExSeep"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="거래수량"
            android:textColor="#000000" />


        <TextView
            android:id="@+id/tv_iExOne"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="거래단가"
            android:textColor="#000000" />


        <TextView
            android:id="@+id/tv_IExMoney"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="거래금액"
            android:textColor="#000000" />


        <TextView
            android:id="@+id/tv_iExFee"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="수수료"
            android:textColor="#000000" />


        <TextView
            android:id="@+id/tv_iExRealMoney"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="정산금액"
            android:textColor="#000000" />


        <TextView
            android:id="@+id/tv_iExOrderTime2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:background="@drawable/gray_border_background"
            android:gravity="center"
            android:text="주문시간"
            android:textColor="#000000" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>